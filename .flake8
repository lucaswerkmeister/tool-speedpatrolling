# -*- mode: conf; -*-
[flake8]
exclude =
    venv
ignore =
    # expected 2 blank lines
    E302 # between functions and classes
    E305 # after functions and classes
    # line too long
    E501
    # imported but unused
    F401
    # local variable is assigned to but never used
    F841
    # line break after binary operator
    W504
